{
  "plugins": ["react", "security"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:security/recommended"
  ],
  "rules": {
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-console": "warn"
  }
}
